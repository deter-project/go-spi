func TestMarshallEnvelope(t *testing.T) {
	e := Envelope{}
	data, err := xml.MarshalIndent(&e, "","  ")
	if err != nil {
		//die slowly and painfully
	}
	buf := bytes.NewBuffer(data)
	fmt.Println(buf.String())
}
